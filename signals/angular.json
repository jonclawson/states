{
  "$schema": "https://angular.io/config/schema.json",
  "version": 1,
  "projects": {
    "analog-signals-sample": {
      "projectType": "application",
      "root": "src",
      "sourceRoot": "src",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/analog-signals-sample",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "tsconfig.json",
            "assets": [],
            "styles": ["src/styles.css"]
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "analog-signals-sample:build"
          }
        }
      }
    }
  },
  "defaultProject": "analog-signals-sample"
}
